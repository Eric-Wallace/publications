	month = apr,
	year = {2008},
	keywords = {Fear, Knowledge documentation, Knowledge sharing, Mediating effect, Trust},
	pages = {206--220},
	file = {science-4:/Users/glasgka1/Library/Application Support/Firefox/Profiles/sicds1nj.default/zotero/storage/8PR2QHA9/science-4:application/pdf;ScienceDirect Full Text PDF:/Users/glasgka1/Library/Application Support/Firefox/Profiles/sicds1nj.default/zotero/storage/N3CTSMDU/Birgit - 2008 - Trust in management and knowledge sharing The med.pdf:application/pdf;ScienceDirect Snapshot:/Users/glasgka1/Library/Application Support/Firefox/Profiles/sicds1nj.default/zotero/storage/RPJX7T3Q/S030504830600096X.html:text/html}
