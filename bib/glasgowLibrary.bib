	note = {{PMID:} 18511683},
	keywords = {Adolescent, Adult, Brain, Brain Mapping, Computational Biology, Female, Humans, language, Magnetic Resonance Imaging, Male, Models, Neurological, Models, Statistical, Semantics, Speech Perception},
	pages = {1191--1195},
	file = {Science-2008-Mitchell-1191-5.pdf:/Users/glasgka1/Library/Application Support/Firefox/Profiles/sicds1nj.default/zotero/storage/4SJIFGBU/Science-2008-Mitchell-1191-5.pdf:application/pdf}
